<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.daftarpelanggaransiswa">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DaftarPelanggaranSiswa"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".CariActivity"
            android:exported="false" />
        <activity
            android:name=".RegistActivity"
            android:exported="false" />
        <activity
            android:name=".ActivityDetail"
            android:exported="false" />
        <activity
            android:name=".listAdapt"
            android:exported="false" />
        <activity
            android:name=".daftar_pelanggaran_siswa"
            android:exported="false"
            android:theme="@style/Theme.Menu"
            android:parentActivityName=".utama"/>
        <activity
            android:name=".InputPelanggaran"
            android:exported="false" />
        <activity
            android:name=".utama"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
    </application>

</manifest>